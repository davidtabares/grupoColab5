@page
@model Ganaderia.App.Presentacion.Pages.ListModel
@{
}
<h1>Listado de Ganaderos</h1>

<table class="table table-hover">
    <tr>
        <th>Nombre</th>
        <th>Apellidos</th>
        <th>Telefono</th>
        <th>Email</th>
        <th>Contraseña</th>
        <th>Rol</th>
        <th>Reg. Fedegan</th>
    </tr>

    @foreach (var ganadero in Model.Ganaderos)
    {
        <tr>
            <td>@ganadero.Nombres</td>
            <td>@ganadero.Apellidos</td>
            <td>@ganadero.NumeroTelefono</td>
            <td>@ganadero.Email</td>
            <td>@ganadero.Contrasena</td>
            <td>@ganadero.Rol</td>
            <td>@ganadero.RegistroFedegan</td>
        </tr>
    }
</table>